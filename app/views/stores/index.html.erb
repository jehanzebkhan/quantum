<p id="notice"><%= notice %></p>

<h1>Stores</h1>

<table id="stores-table" class="stores-table table table-striped">
  <thead>
    <tr>
      <th>Name</th>
    </tr>
  </thead>

  <tbody>
    <% @stores.each do |store| %>
      <tr>
        <td><%= store.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Store', new_store_path,  class: "btn btn-primary" %>
